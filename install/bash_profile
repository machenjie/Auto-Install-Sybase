export SYBASE=$INSTALL_DIR
export DSQUERY=${SERVER_NAME}
export SYBASE_OCS=${SYBASE_OCS}
export SYBASE_ASE=${SYBASE_ASE}
export PATH=$PATH:${INSTALL_DIR}:${INSTALL_DIR}/$SYBASE_ASE/bin:$INSTALL_DIR/$SYBASE_ASE/install:$INSTALL_DIR/$SYBASE_OCS/bin:$INSTALL_DIR/bin:$INSTALL_DIR/bin:/sbin:.
export SHLIB_PATH=$SHLIB_PATH:$INSTALL_DIR/$SYBASE_ASE/lib:$INSTALL_DIR/$SYBASE_OCS/lib:$INSTALL_DIR/$SYBASE_OCS/lib3p64:$INSTALL_DIR/$SYBASE_OCS/lib3p:$INSTALL_DIR/lib:/usr/lib:$SHLIB_PATH
export LIB_PATH=$INSTALL_DIR/$SYBASE_ASE/lib:$INSTALL_DIR/$SYBASE_OCS/lib:$INSTALL_DIR/$SYBASE_OCS/lib3p64:$INSTALL_DIR/$SYBASE_OCS/lib3p:$INSTALL_DIR/lib:/usr/lib:$LIB_PATH
export LD_LIBRARY_PATH=$INSTALL_DIR/$SYBASE_ASE/lib:$INSTALL_DIR/$SYBASE_OCS/lib:$INSTALL_DIR/$SYBASE_OCS/lib3p64:$INSTALL_DIR/$SYBASE_OCS/lib3p:$INSTALL_DIR/lib:/usr/lib:${LD_LIBRARY_PATH}
export LANG=C
